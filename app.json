{
  "name": "Pyrogram And Telethon String Session Generator",
  "description": "Generate Pyrogram And Telethon String Session Easley",
  "logo": "https://telegra.ph/file/27bfcfbb9d72fb3f3a1ca.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
 "repository": "https://github.com/mikeel-ye/Session",
 "stack": "heroku-22",
 "addons": [
    {
       "options": {
          "version": "15"
       },
       "plan": "heroku-postgresql"
    }
 ],
  "env": {
    "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "Obtain a Telegram bot token by contacting @BotFather",
      "required": true,
      "value": ""
    },
    "MUST_JOIN": {
      "description": "Add username/id of a channel/group",
      "required": false,
      "value": "https://t.me/Alexa_Help"
    }
  }
}